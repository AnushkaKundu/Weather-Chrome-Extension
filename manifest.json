{
    "manifest_version": 2,
    "name": "Weather Extension",
    "version": "1.0",
    "description": "Displays weather information for your location",
    "permissions": ["geolocation"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  